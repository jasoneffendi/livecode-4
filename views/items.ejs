<html>
        <body>
            <table>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Brand</th>
                    <th>Code Item</th>
                </tr>
                
                    <%data.forEach(d => {%>
                        <tr>
                            <td><%=d.id%></td>
                            <td><%=d.name%></td>
                            <td><%=d.brand%></td>
                            <td><%=d.codeitem%></td>
                            <td><a href='/item/edit/<%=d.id%>'>Edit</a></td>
                            <td><a href='/item/delete/<%=d.id%>'>Delete</a></td>
                        </tr>
                       
                    <%})%>
            </table>
            <a href='/item/add'>Add an item</a>
            <a href='/'>Home</a>
        </body>
    </html>